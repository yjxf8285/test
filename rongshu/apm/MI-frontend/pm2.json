{
  "apps": [{
    "name": "mapm",
    "script": "admin/index.js",
    "log_date_format": "YYYY-MM-DD HH:mm:SS",
    "out_file": "./logs/mapm-out.log",
    "error_file": "./logs/mapm-err.log",
    "pid_file": "./logs/mapm.pid",
    "exec_mode": "cluster",
    "instances": 2,
    "env": {
      "NODE_ENV": "development"
    },
    "env_test": {
      "NODE_ENV": "test"
    },
    "env_production": {
      "NODE_ENV": "production"
    },
    "env_uat": {
      "NODE_ENV": "uat"
    }
  }]
}
