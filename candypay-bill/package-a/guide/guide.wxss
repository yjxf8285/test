.bj-wrap {
    position: relative;
    background: #fff;
    border-top: solid 1rpx #e0e0e0;
}
.title {
    text-align: center;
    font-size: 36rpx;
    height: 90rpx;
    line-height: 90rpx;
    font-weight: bold;
}
.btn {
    position: absolute;
    top: 94rpx;
    right: 20rpx;
    width: 96rpx;
    height: 35rpx;
}
.bottom-wrap {
    position: fixed;
    bottom: 0;
    left: 0;
    right:0;
    background: #fff;
    height: 164rpx;
    text-align: center;
    border-top: solid 1px rgba(153, 153, 153, 1);
    display: flex;
    justify-content: center;
    align-items: center;
    flex-flow: column;
}
.bottom-wrap .gohome-btn {
    margin: 0rpx;
    width: 220rpx;
    height: 60rpx;
    line-height: 60rpx;
    color: #fff;
    background-color: rgba(255, 102, 102, 1);
    font-size: 26rpx;
    border-radius: 100rpx;
}
.bottom-wrap .tips {
    font-size: 22rpx;
    color: #999999;
}
