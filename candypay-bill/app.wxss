/**app.wxss**/
@import '/style/font-icon.wxss';
@import '/style/common.wxss';

page {
    background-color: #e9e9e9;
    font-size: 32rpx;
    height: 100%;
    line-height: 44rpx;
}
.container {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
}

/* 全局开关样式重置 */

.wx-switch-input {
    zoom: 0.8;
}
